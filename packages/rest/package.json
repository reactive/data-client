{
  "name": "@rest-hooks/rest",
  "version": "2.1.4",
  "description": "Endpoints for REST APIs",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "lib/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    ">=4.0": {
      "": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/index.d.ts"
      ]
    },
    ">=3.4": {
      "": [
        "ts3.4/index.d.ts"
      ],
      "*": [
        "ts3.4/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "lib",
    "ts3.4",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:lib": "cross-env NODE_ENV=production  BROWSERSLIST_ENV=2019 ROOT_PATH_PREFIX='@rest-hooks/core' babel --root-mode upward src --out-dir lib --source-maps inline --extensions '.ts,.tsx,.js' --ignore '**/__tests__/**' --ignore '**/*.d.ts'",
    "build:bundle": "rollup -c",
    "build:clean": "rimraf lib dist ts3.4 *.tsbuildinfo",
    "build:legacy-types": "yarn run downlevel-dts lib ts3.4",
    "build": "yarn run build:lib && yarn run build:bundle",
    "dev": "yarn run build:lib -w",
    "prepare": "yarn run build:lib",
    "prepublishOnly": "yarn run build:bundle",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "yarn test -- --ci",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage"
  },
  "keywords": [
    "REST",
    "CRUD",
    "networking",
    "fetch",
    "data",
    "cache",
    "model",
    "resource",
    "typescript",
    "data fetching",
    "data cache",
    "api",
    "normalized cache",
    "subject pattern",
    "endpoint",
    "json"
  ],
  "author": "Nathaniel Tucker <me@ntucker.me> (https://github.com/ntucker)",
  "license": "Apache-2.0",
  "homepage": "https://resthooks.io/docs/api/resource",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:coinbase/rest-hooks.git",
    "directory": "packages/rest"
  },
  "bugs": {
    "url": "https://github.com/coinbase/rest-hooks/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2"
  },
  "peerDependencies": {
    "@rest-hooks/endpoint": "^0.6.1 || ^1.0.0"
  }
}
