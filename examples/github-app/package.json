{
  "name": "github-app",
  "version": "0.4.15",
  "description": "github-app - A reactive data client example",
  "scripts": {
    "lint": "yarn g:lint src --ext .ts,.tsx",
    "format": "run lint --fix",
    "test:type": "run tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "yarn dlx serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "stackblitz": {
    "startCommand": "npm start"
  },
  "engines": {
    "node": ">=18.0"
  },
  "browserslist": "extends @anansi/browserslist-config",
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "6.2.15",
    "@anansi/browserslist-config": "1.7.3",
    "@anansi/webpack-config": "21.0.12",
    "@babel/core": "7.28.4",
    "@linaria/babel-preset": "5.0.4",
    "@linaria/core": "6.3.0",
    "@linaria/react": "6.3.0",
    "@linaria/shaker": "5.0.3",
    "@types/lodash": "4.14.192",
    "@types/parse-link-header": "^2.0.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/uuid": "^10.0.0",
    "react-refresh": "0.17.0",
    "serve": "14.2.5",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "dependencies": {
    "@anansi/router": "0.10.17",
    "@ant-design/icons": "^5.0.0",
    "@babel/runtime-corejs3": "^7.26.7",
    "@data-client/graphql": "^0.14.0",
    "@data-client/img": "^0.14.15",
    "@data-client/react": "^0.14.0",
    "@data-client/rest": "^0.14.0",
    "@js-temporal/polyfill": "^0.5.0",
    "antd": "5.27.5",
    "core-js": "^3.40.0",
    "history": "^5.3.0",
    "parse-link-header": "^2.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-markdown": "10.1.0",
    "rehype-highlight": "7.0.2",
    "remark-gfm": "^4.0.0",
    "remark-remove-comments": "^1.0.1",
    "uuid": "^11.0.0"
  },
  "packageManager": "yarn@4.10.3"
}
