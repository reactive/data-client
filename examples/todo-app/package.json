{
  "name": "todo-app",
  "version": "0.2.24",
  "description": "todo-app - A reactive data client example",
  "scripts": {
    "lint": "yarn g:lint src --ext .ts,.tsx",
    "format": "run lint --fix",
    "test:type": "tsc",
    "start": "webpack serve --mode=development",
    "start:prod": "yarn dlx serve dist",
    "build:browser": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "stackblitz": {
    "startCommand": "npm start"
  },
  "browserslist": "extends @anansi/browserslist-config",
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@anansi/babel-preset": "6.2.22",
    "@anansi/browserslist-config": "1.7.4",
    "@anansi/webpack-config": "21.1.12",
    "@babel/core": "7.28.6",
    "@linaria/core": "6.3.0",
    "@linaria/react": "6.3.0",
    "@linaria/shaker": "5.0.3",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "^11.0.0",
    "react-refresh": "0.18.0",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.7",
    "@data-client/endpoint": "0.15.5",
    "@data-client/react": "0.15.4",
    "@data-client/rest": "0.15.5",
    "core-js": "^3.40.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "uuid": "^13.0.0"
  }
}
